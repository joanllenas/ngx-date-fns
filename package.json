{
  "name": "ngx-date-fns",
  "version": "4.0.2",
  "description": "date-fns pipes for Angular",
  "repository": {
    "type": "git",
    "url": "https://github.com/joanllenas/ngx-date-fns"
  },
  "keywords": [
    "angular",
    "ngx",
    "angular2",
    "ng2",
    "date-fns",
    "date"
  ],
  "author": {
    "name": "Joan Llenas MasÃ³",
    "email": "joan.llenas.maso@gmail.com",
    "url": "http://joanllenas.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joanllenas/ngx-date-fns/issues"
  },
  "homepage": "https://github.com/joanllenas/ngx-date-fns#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-date-fns",
    "test": "ng test ngx-date-fns",
    "lint": "ng lint ngx-date-fns",
    "prettier": "prettier --write src/app/**/*.ts && prettier --write projects/ngx-date-fns/src/lib/**/*.ts",
    "precommit": "npm run lint && npm run test -- --watch=false",
    "e2e": "ng e2e",
    "cm": "git-cz",
    "cm-retry": "git-cz --retry",
    "build:version": "sync-json -v --property version --source package.json projects/ngx-date-fns/package.json && sync-json -v --property version --source package.json dist/ngx-date-fns/package.json",
    "release": "npm run build && standard-version --no-verify --commit-all && npm publish dist/ngx-date-fns",
    "next": "npm run build && standard-version --no-verify --commit-all && npm publish dist/ngx-date-fns --tag next",
    "standard-version": "standard-version"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run build:version",
      "precommit": "git add .",
      "posttag": "git push --follow-tags"
    }
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^3.0.0",
    "date-fns": "^1.29.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular-devkit/build-ng-packagr": "~0.12.0",
    "@angular/cli": "~7.2.2",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "commitizen": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.16.0",
    "protractor": "~5.4.0",
    "standard-version": "^4.4.0",
    "sync-json": "^1.0.2",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
