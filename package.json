{
  "name": "ngx-date-fns",
  "version": "3.0.2",
  "description": "date-fns pipes for Angular",
  "repository": {
    "type": "git",
    "url": "https://github.com/joanllenas/ngx-date-fns"
  },
  "keywords": [
    "angular",
    "ngx",
    "angular2",
    "ng2",
    "date-fns",
    "date"
  ],
  "author": "Joan Llenas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joanllenas/ngx-date-fns/issues"
  },
  "homepage": "https://github.com/joanllenas/ngx-date-fns#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage": "ng test --watch=false --code-coverage && codecov",
    "cm": "git-cz",
    "cm-retry": "git-cz --retry",
    "build:lib": "gulp --gulpfile gulpfile.lib.js",
    "build:version": "sync-json -v --property version --source package.json src/lib/package.json && sync-json -v --property version --source package.json dist/package.json",
    "release": "npm run build:lib && standard-version --no-verify --commit-all && npm publish dist",
    "next": "npm run build:lib && standard-version --no-verify --commit-all && npm publish dist --tag next",
    "standard-version": "standard-version"
  },
  "standard-version": {
    "scripts": {
      "postbump": "npm run build:version",
      "precommit": "git add .",
      "posttag": "git push --follow-tags"
    }
  },
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.4.1",
    "date-fns": "^2.0.0-alpha.16",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.3",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~9.6.2",
    "codecov": "^3.0.0",
    "codelyzer": "^4.0.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "fs": "0.0.1-security",
    "ghooks": "^2.0.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "path": "^0.12.7",
    "protractor": "~5.4.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.0",
    "run-sequence": "^2.2.1",
    "sorcery": "^0.10.0",
    "standard-version": "^4.3.0",
    "sync-json": "^1.0.2",
    "through2": "^2.0.3",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test -- --watch=false"
    }
  }
}
