{
  "name": "ngx-date-fns",
  "version": "0.0.0-development",
  "description": "date-fns pipes for Angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm run lint",
    "build": "ng build",
    "test": "ng test",
    "coverage": "ng test --watch=false --code-coverage",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release pre && npm publish dist/ --tag=next && semantic-release post",
    "cm": "git-cz",
    "packagr": "ng-packagr -p ng-package.json"
  },
  "release": {
    "branch": "develop"
  },
  "publishConfig": {
    "tag": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joanllenas/ngx-date-fns"
  },
  "keywords": [
    "angular",
    "ngx",
    "angular2",
    "ng2",
    "date-fns",
    "date"
  ],
  "author": "Joan Llenas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joanllenas/ngx-date-fns/issues"
  },
  "homepage": "https://github.com/joanllenas/ngx-date-fns#readme",
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "core-js": "^2.4.1",
    "date-fns": "1.29.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.5.4",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "~5.1.2",
    "semantic-release": "^8.2.0",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test -- --watch=false"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
